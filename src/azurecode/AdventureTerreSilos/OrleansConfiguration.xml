<?xml version="1.0" encoding="utf-8"?>
<OrleansConfiguration xmlns="urn:orleans">
  <Globals>
    <Liveness LivenessType="AzureTable" />
    <StorageProviders>
      <Provider Type="Orleans.Storage.MemoryStorage" Name="MemoryStore" />
      <Provider Type="Orleans.Storage.AzureTableStorage" Name="AzureStore" DataConnectionString="DefaultEndpointsProtocol=https;AccountName=**NAME**;AccountKey=**KEY**" />
    </StorageProviders>
  </Globals>
	<Defaults>
    <Tracing DefaultTraceLevel="Info" TraceToConsole="true" TraceToFile="c:\Logs\{0}-{1}.log" >
    </Tracing>
	</Defaults>
</OrleansConfiguration>
